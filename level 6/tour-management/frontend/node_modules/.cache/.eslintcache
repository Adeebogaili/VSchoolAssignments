end data to the server\r\n  const handleSubmit = (e) => {\r\n    e.preventDefault();\r\n\r\n    navigate('/thank-you');\r\n  };\r\n  return (\r\n    <div className='booking'>\r\n      <div className='booking__top d-flex align-content-center justify-content-between'>\r\n        <h3>\r\n          ${price} <span>/person</span>\r\n        </h3>\r\n        <span className='tour__rating d-flex align-items-center justify-content-between'>\r\n          <i className='ri-star-fill'></i> {avgRating === 0 ? null : avgRating}{' '}\r\n          ({reviews?.length})\r\n        </span>\r\n      </div>\r\n      {/* booking from start */}\r\n      <div className='booking__form'>\r\n        <h5>Information</h5>\r\n        <Form className='booking__info-form' onSubmit={handleSubmit}>\r\n          <FormGroup>\r\n            <input\r\n              type='text'\r\n              placeholder='Full Name'\r\n              id='fullName'\r\n              required\r\n              onChange={handleChange}\r\n            />\r\n          </FormGroup>\r\n          <FormGroup>\r\n            <input\r\n              type='number'\r\n              placeholder='Phone'\r\n              id='phone'\r\n              required\r\n              onChange={handleChange}\r\n            />\r\n          </FormGroup>\r\n          <FormGroup className='d-flex align-items-center gap-3'>\r\n            <input\r\n              type='date'\r\n              placeholder=''\r\n              id='bookAt'\r\n              required\r\n              onChange={handleChange}\r\n            />\r\n            <input\r\n              type='number'\r\n              placeholder='Guest'\r\n              id='guestSize'\r\n              required\r\n              onChange={handleChange}\r\n            />\r\n          </FormGroup>\r\n        </Form>\r\n      </div>\r\n      {/* booking from end */}\r\n      {/* booking bottom */}\r\n      <div className='booking__bottom'>\r\n        <ListGroup>\r\n          <ListGroupItem className='border-0 px-0'>\r\n            <h5 className='d-flex align-items-center gap-1'>\r\n              ${price} <i className='ri-close-line'></i> {credentials.guestSize}{' '}\r\n              person\r\n            </h5>\r\n            <span>${price}</span>\r\n          </ListGroupItem>\r\n          <ListGroupItem className='border-0 px-0'>\r\n            <h5>Service Charge</h5>\r\n            <span>${serviceFee}</span>\r\n          </ListGroupItem>\r\n          <ListGroupItem className='border-0 px-0 total'>\r\n            <h5>Total</h5>\r\n            <span>${totalAmount}</span>\r\n          </ListGroupItem>\r\n        </ListGroup>\r\n\r\n        <Button className='btn primary__btn w-100 mt-4' onClick={handleSubmit}>\r\n          Book Now\r\n        </Button>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Booking;\r\n","/home/adeeb/development/assignments/level 6/tour-management/frontend/src/pages/ThankYou.jsx",[],[],"/home/adeeb/development/assignments/level 6/tour-management/frontend/src/utils/config.js",[],[],"/home/adeeb/development/assignments/level 6/tour-management/frontend/src/hooks/useFetch.js",[],[],{"ruleId":"158","severity":1,"message":"159","line":3,"column":48,"nodeType":"160","messageId":"161","endLine":3,"endColumn":52},{"ruleId":"158","severity":1,"message":"162","line":15,"column":10,"nodeType":"160","messageId":"161","endLine":15,"endColumn":20},{"ruleId":"158","severity":1,"message":"163","line":41,"column":11,"nodeType":"160","messageId":"161","endLine":41,"endColumn":21},{"ruleId":"158","severity":1,"message":"164","line":10,"column":10,"nodeType":"160","messageId":"161","endLine":10,"endColumn":21},{"ruleId":"158","severity":1,"message":"164","line":10,"column":10,"nodeType":"160","messageId":"161","endLine":10,"endColumn":21},{"ruleId":"158","severity":1,"message":"159","line":9,"column":3,"nodeType":"160","messageId":"161","endLine":9,"endColumn":7},"no-unused-vars","'List' is defined but never used.","Identifier","unusedVar","'tourRat